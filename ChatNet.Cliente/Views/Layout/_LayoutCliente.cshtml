<!DOCTYPE html>
<html lang="en" style="height: auto;">

<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewData["Title"]</title>

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <link rel="stylesheet" href="~/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/dist/css/adminlte.min.css">
    <link href="~/css/site.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/dist/img/logo.jpeg" />

    <!-- jQuery UI -->
    <link href="~/plugins/jquery-ui/jquery-ui.min.css" rel="stylesheet" />
    <!-- Sweetalert 2 -->
    <link href="~/plugins/sweetalert2/sweetalert2.min.css" rel="stylesheet" />

    @await RenderSectionAsync("Styles", required: true)

    <!-- jQuery -->
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI -->
    <script src="~/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Sweetalert 2 -->
    <script src="~/plugins/sweetalert2/sweetalert2.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE -->
    <script src="~/dist/js/adminlte.js"></script>
    <!-- Custon File Inputs -->
    <script src="~/plugins/bs-custom-file-input/bs-custom-file-input.min.js"></script>
    <script>
        $(function () {
            bsCustomFileInput.init();
        });

        const Alertas = (icono,titulo,texto) =>{
            Swal.fire({
                title: titulo,
                html: texto,
                icon: icono
            });
        }
    </script>
    <!-- Loadingoverlay -->
    <script src="~/lib/loadingoverlay/loadingoverlay.min.js"></script>

    @await RenderSectionAsync("Scripts", required: true)
</head>

<body>

    @RenderBody()

</body>

</html>

