<form autocomplete="off" method="post" enctype="multipart/form-data" action="javascript:void(0)">
    <div>
        <div class="float-left text-success">
            <i class="fas fa-chevron-left" style="cursor:pointer;" onclick="LoginUsuario()"></i>
        </div>
        <div class="text-center">
            <h4 class="text-success">Datos del Usuario</h4>
        </div>
    </div>
    
    <hr />

    <div class="row g-3 justify-content-around">
        <div class="col">
            <div class="form-group">
                <label for="nombre" class="form-label text-success">Nombre</label>
                <input type="text" class="form-control text-success bg-dark border-success" id="nombre" name="nombre" placeholder="Nombre" />
            </div>
        </div>
        <div class="col">
            <div class="form-group">
                <label for="apellido" class="form-label text-success">Apellido</label>
                <input type="text" class="form-control text-success bg-dark border-success" id="apellido" name="apellido" placeholder="Apellido" />
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="correo" class="form-label text-success">Correo Eléctronico</label>
        <input type="text" class="form-control text-success bg-dark border-success" id="correo" name="correo" placeholder="Correo Eléctronico" />
    </div>

    <div class="form-group">
        <label for="password" class="form-label text-success">Contraseña</label>
        <input type="password" class="form-control text-success bg-dark border-success" id="password" name="password" placeholder="Contraseña" />
    </div>

    <div class="text-center">
        <button type="submit" class="btn btn btn-outline-dark border-success text-success">Registrar</button>
    </div>
</form>